{
  "expo": {
    "name": "Fantasy AI",
    "slug": "fantasyai",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/crystalball.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fantasyai.app",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "com.android.vending.BILLING"
      ],
       "versionCode": 4,
      "package": "com.fantasyai.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "fantasyai",
              "host": "fantasyai.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "plugins": [
      "expo-build-properties",
      "@react-native-google-signin/google-signin",
      "@react-native-purchases/expo-plugin",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.fantasyai.app",
          "enableGooglePay": true
        }
      ]
    ],
    "scheme": "fantasyai",
    "extra": {
       "eas": {
        "projectId": "518dbf1d-122d-4f86-9940-1ef2f1137a2e"
